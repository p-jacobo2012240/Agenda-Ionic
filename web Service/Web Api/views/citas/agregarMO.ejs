        <select class="form-control" style="margin: 3px 10px" ng-model="contactoCI" ng-options="con as con.nombre for con in contactosList"></select>
        <textarea type="text" class="form-control" style="margin: 3px 10px" ng-model="descripcionCI" placeholder="Descripcion" ></textarea>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-sm btn-outline-secondary" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-sm btn-outline-primary">AÃ±adir Categoria</button>
      </div>
      </form>
    </div>
  </div>
</div>