    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Agregar</h5>
        <button type="button" class="close" id="closeAGUSMO" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <form ng-submit="gestionCI(1)">
      <div class="modal-body">
        <input type="text" class="form-control" style="margin: 3px 10px" ng-model="lugarCI" placeholder="Lugar" required/>
